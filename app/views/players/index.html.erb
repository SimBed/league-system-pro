<div class="container px-1">
  <% content_for :title, "Players" %>
  <h5 class='mb-4'>Players 
    <%= link_to new_player_path, data: { turbo_frame: dom_id(Player.new)} do %>
      <i class="bi bi-database-fill-add"></i>
    <% end %>
  </h5>
  <%= turbo_frame_tag Player.new %>
  <%= turbo_frame_tag 'players' do %>
    <%= render 'players_table' %>
  <% end %>
</div>